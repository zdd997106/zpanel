{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "declarationMap": true,
    "paths": {
      "src/*": ["src/*"]
    },
    "outDir": "./dist"
  }
}
